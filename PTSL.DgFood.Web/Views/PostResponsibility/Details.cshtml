@model PTSL.DgFood.Web.Model.PostResponsibilityVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Post Responsibility Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputPostResponsibility" style="font-size:15px;">Post Responsibility Name: <b>@Model.PostResponsibilityName</b></label>
                @*<input type="text" class="form-control col-md-6" id="PostResponsibilityName" name="PostResponsibilityName" value="@Model.PostResponsibilityName" placeholder="Enter PostResponsibility Name">*@
            </div>
            <div class="form-group">
                <label for="inputPostResponsibilityBn" style="font-size:15px;">Post Responsibility Name Bangla: <b>@Model.PostResponsibilityNameBn</b></label>
            </div>
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/PostResponsibility/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

