<script>
    $("#EmployeeInformationVM_PassportIssueDate").change(function () {
        var PassportIssueDate = document.getElementById("EmployeeInformationVM_PassportIssueDate").value;
        var PassportDateOfExpire = document.getElementById("EmployeeInformationVM_PassportDateOfExpire").value;
        if ((Date.parse(PassportIssueDate) > Date.parse(PassportDateOfExpire))) {
            alert("Passport Issue Date can not be later than Passport Expire Date");
            document.getElementById("EmployeeInformationVM_PassportIssueDate").value = "";
        }
    });

    $("#EmployeeInformationVM_PassportDateOfExpire").change(function () {
        var PassportIssueDate = document.getElementById("EmployeeInformationVM_PassportIssueDate").value;
        var PassportDateOfExpire = document.getElementById("EmployeeInformationVM_PassportDateOfExpire").value;
        if ((Date.parse(PassportIssueDate) > Date.parse(PassportDateOfExpire))) {
            alert("Passport Expire Date can not be earlier than Passport Issue Date");
            document.getElementById("EmployeeInformationVM_PassportDateOfExpire").value = "";
        }
    });

    $("#TrainingInformationVM_FromDate").change(function () {
        var TrainingInformationVM_FromDate = document.getElementById("TrainingInformationVM_FromDate").value;
        var TrainingInformationVM_ToDate = document.getElementById("TrainingInformationVM_ToDate").value;
        if ((Date.parse(TrainingInformationVM_FromDate) > Date.parse(TrainingInformationVM_ToDate))) {
            alert("From date should be less or equal To date");
            document.getElementById("TrainingInformationVM_FromDate").value = "";
        }
    });

    $("#TrainingInformationVM_ToDate").change(function () {
        var TrainingInformationVM_FromDate = document.getElementById("TrainingInformationVM_FromDate").value;
        var TrainingInformationVM_ToDate = document.getElementById("TrainingInformationVM_ToDate").value;
        if ((Date.parse(TrainingInformationVM_FromDate) > Date.parse(TrainingInformationVM_ToDate))) {
            alert("To date should be greater or equal From date");
            document.getElementById("TrainingInformationVM_ToDate").value = "";
        }
    });

       $("#tabs").tabs({
        active: @ViewBag.TabIndex,
        activate: function (event, ui) {
            var active = $('#tabs').tabs('option', 'active');
        }
       });

    $(document).ready(function () {
        var CollapseValue = @Html.Raw(Json.Encode(ViewBag.CollapseValue));
        if (CollapseValue == "collapseSpouse") {
            $('#collapseSpouse').collapse({
                toggle: true
            });
            LoadSpouseData();
        }
        else if (CollapseValue == "collapseChild") {
            $('#collapseChild').collapse({
                toggle: true
            });
            LoadChildData()
        }
        else if (CollapseValue == "collapsePresentAddress") {
            $('#collapsePresentAddress').collapse({
                toggle: true
            });
            LoadPresentAddressData();
        }
        else if (CollapseValue == "collapsePermanentAddress") {
            $('#collapsePermanentAddress').collapse({
                toggle: true
            });
            LoadParmanentAddressData();
        }
        else if (CollapseValue == "collapseEducationalQualification") {
            $('#collapseEducationalQualification').collapse({
                toggle: true
            });
            LoadEducationalQualificationData();
        }
        else if (CollapseValue == "collapseTrainingInformation") {
            $('#collapseTrainingInformation').collapse({
                toggle: true
            });
