@model PTSL.DgFood.Web.Model.DivisionVM
@{
    ViewBag.Title = "Details";
}

<div class="card">
    <div class="card-header">
        <h4>Division Details</h4>
    </div>
    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputDivision" style="font-size:15px;">Division Name: <b>@Model.DivisionName</b></label>
                @*<input type="text" class="form-control col-md-6" id="DivisionName" name="DivisionName" value="@Model.DivisionName" placeholder="Enter Division Name">*@
            </div>
            <div class="form-group">
                <label for="inputDivisionBangla" style="font-size:15px;">Division Name Bangla: <b>@Model.DivisionNameBangla</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Geo Code: <b>@Model.GeoCode</b></label>
            </div>
        </div>
    </div>

    <hr />
    <div class="card-header pull-left">
        <a href="/Division/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

