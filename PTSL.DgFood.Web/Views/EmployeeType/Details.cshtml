@model PTSL.DgFood.Web.Model.EmployeeTypeVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Employee Type Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputEmployeeType" style="font-size:15px;">Employee Type Name: <b>@Model.EmployeeTypeName</b></label>
                @*<input type="text" class="form-control col-md-6" id="EmployeeTypeName" name="EmployeeTypeName" value="@Model.EmployeeTypeName" placeholder="Enter EmployeeType Name">*@
            </div>
            <div class="form-group">
                <label for="inputEmployeeTypeBn" style="font-size:15px;">Employee Type Name Bangla: <b>@Model.EmployeeTypeNameBn</b></label>
            </div>
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/EmployeeType/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

