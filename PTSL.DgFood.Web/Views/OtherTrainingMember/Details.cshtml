@model PTSL.DgFood.Web.Model.EntityViewModels.EmployeeManagementEntity.OtherTrainingMemberVM
@{
    ViewBag.Title = "Details";
}

<div class="card">
    <div class="card-header">
        <h4>Other Training Member Details</h4>
    </div>
    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputDivision" style="font-size:15px;">Name: <b>@Model.Name</b></label>
                @*<input type="text" class="form-control col-md-6" id="DivisionName" name="DivisionName" value="@Model.DivisionName" placeholder="Enter Division Name">*@
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Email: <b>@Model.Email</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Phone: <b>@Model.Phone</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Address Or Workplace: <b>@Model.AddressOrWorkplace</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Gender: <b>@Model.GenderId</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Training Plan: <b>@Model.TrainingManagementType.TrainingPlan</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Training Title: <b>@Model.TrainingManagementType.TrainingTitle</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Training Batch: <b>@Model.TrainingManagementType.TrainingBatch</b></label>
            </div>
            <div class="form-group">
                <label for="inputGeoCode" style="font-size:15px;">Training Location Type: <b>@Model.TrainingManagementType.TrainingLocationType</b></label>
            </div>
        </div>
    </div>

    <hr />
    <div class="card-header pull-left">
        <a href="/OtherTrainingMember/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

