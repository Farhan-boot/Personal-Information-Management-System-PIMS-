@model PTSL.DgFood.Web.Model.DistrictVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>District Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <table style="width:100%">
                <tr>
                    <td style="width:50px;">Division Name: <b>@Model.Division.DivisionName</b></td>
                    <td style="width:50px;">District Name: <b>@Model.DistrictName</b></td>
                </tr>
            </table>
            
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/District/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

