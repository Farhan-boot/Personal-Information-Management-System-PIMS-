@model PTSL.DgFood.Web.Model.JobCategoryVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Job Category Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputJobCategory" style="font-size:15px;">Job Category Name: <b>@Model.JobCategoryName</b></label>
                @*<input type="text" class="form-control col-md-6" id="JobCategoryName" name="JobCategoryName" value="@Model.JobCategoryName" placeholder="Enter JobCategory Name">*@
            </div>
            <div class="form-group">
                <label for="inputJobCategoryBn" style="font-size:15px;">Job Category Name Bangla: <b>@Model.JobCategoryNameBn</b></label>
            </div>
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/JobCategory/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

