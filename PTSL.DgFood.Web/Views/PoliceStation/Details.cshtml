@model PTSL.DgFood.Web.Model.PoliceStationVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Police Station Details</h4>
    </div>

    <div class="card-body">
        <table style="width:100%">
            <tr>
                <td style="width:50px;">District Name: <b>@Model.DistrictDTO.DistrictName</b></td>
                <td style="width:50px;">Police Station Name: <b>@Model.PoliceStationName</b></td>
                <td style="width:50px;">Police Station Name Bangla: <b>@Model.PoliceStationNameBangla</b></td>
            </tr>
        </table>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/PoliceStation/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

