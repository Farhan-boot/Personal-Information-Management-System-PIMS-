<script>

    var chart4Completed = [];
    var chart4Total = [];
    var chart4InCompleted = [];
    var chart4Categories = [];

    var chart2Completed = [];
    var chart2Total = [];
    var chart2InCompleted = [];
    var chart2Categories = [];

    var totalRoutesCreated = @Html.Raw(Json.Encode(ViewBag.totalRoutesCreated));
    var totalCompleted = @Html.Raw(Json.Encode(ViewBag.totalCompleted));
    var totalIncompleted = @Html.Raw(Json.Encode(ViewBag.totalIncompleted));
    var routesData = @Html.Raw(Json.Encode(ViewBag.data));
    var dayWisedata = @Html.Raw(Json.Encode(ViewBag.dayWisedata));

    $.each(routesData, function (index, value) {
        //alert(value.UserName);
        chart4Completed.push(value.Completed);
        chart4Total.push(value.Total);
        chart4InCompleted.push(value.NotCompleted);
        chart4Categories.push(value.UserName);
    });

    $.each(dayWisedata, function (index, value) {
        console.log(value.CreatedAt);
        //alert(value.UserName);
        chart2Completed.push(value.Completed);
        chart2Total.push(value.Total);
        chart2InCompleted.push(value.NotCompleted);
        chart2Categories.push(value.CreatedAt +"  GMT");
    });

    $(document).ready(function () {
        //chart4N();
        //chart3N();
        //chart2N();
        //chart1N();
        barChartData();
        pieChartData();
        //pieChartN();
    });

    function chart4N() {
        var options = {
            chart: {
                height: 250,
                type: 'area',
                toolbar: {
                    show: false
                },

            },
            colors: ['#999b9c', '#4CC2B0'], // line color
            fill: {
                colors: ['#999b9c', '#4CC2B0'] // fill color
            },
            dataLabels: {
                enabled: false
            },
            stroke: {
                curve: 'smooth'
            },
            markers: {
                colors: ['#999b9c', '#4CC2B0'] // marker color
            },
            series: [{
                name: 'Completed',
                data: chart4Completed
            }, {
                name: 'InCompleted',
                data: chart4InCompleted
            }],
            legend: {
                show: false,
            },
            xaxis: {
                categories: chart4Categories,
                labels: {
                    style: {
                        colors: "#9aa0ac"
                    }
                },
            },
            yaxis: {
                labels: {
                    style: {
                        color: "#9aa0ac"
                    }
                }
            },
        }

        var chart = new ApexCharts(
            document.querySelector("#chart4"),
            options
        );

        chart.render();

    }

    function chart3N() {
        var options = {
            chart: {
                height: 250,
                type: 'line',
                zoom: {
                    enabled: false
                },
                toolbar: {
                    show: false
                },

            },
            dataLabels: {
                enabled: false
            },
            stroke: {
                width: [5, 7, 5],
                curve: 'straight',
                dashArray: [0, 8, 5]
            },
            series: [{
                name: "Total",
                data: chart4Total
            },
            {
                name: "Completed",
                data: chart4Completed
            },
            {
                name: 'InCompleted',
                data: chart4InCompleted
            }
            ],
            legend: {
                show: false,
            },
            markers: {
                size: 0,

                hover: {
                    sizeOffset: 6
                }
            },
            xaxis: {
                categories: chart4Categories,
                labels: {
                    style: {
                        colors: "#9aa0ac"
                    }
                }
            },
            yaxis: {
                labels: {
                    style: {
                        color: "#9aa0ac"
                    }
                }
            },
            tooltip: {

            },
            grid: {
                borderColor: '#f1f1f1',
            }
        }

        var chart = new ApexCharts(
            document.querySelector("#chart3"),
            options
        );

        chart.render();
    }

    function chart2N() {
        var options = {
            chart: {
                height: 250,
                type: 'bar',
                stacked: true,
                toolbar: {
                    show: false
                },
                zoom: {
                    enabled: true
                }
            },
            responsive: [{
                breakpoint: 480,
