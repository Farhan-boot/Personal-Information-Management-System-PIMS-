@model PTSL.DgFood.Web.Model.GradationTypeVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Gradation Type Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputGradationType" style="font-size:15px;">Gradation Type Name: <b>@Model.GradationTypeName</b></label>
                @*<input type="text" class="form-control col-md-6" id="GradationTypeName" name="GradationTypeName" value="@Model.GradationTypeName" placeholder="Enter GradationType Name">*@
            </div>
            <div class="form-group">
                <label for="inputGradationTypeBn" style="font-size:15px;">Gradation Type Name Bangla: <b>@Model.GradationTypeNameBn</b></label>
            </div>
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/GradationType/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

