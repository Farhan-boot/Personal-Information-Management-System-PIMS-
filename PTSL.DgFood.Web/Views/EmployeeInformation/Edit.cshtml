<script>
    function editEmployeePostingDetails(id) {

        ajax1Promise = $.ajax({
            url: "/EmployeePostingDetails/GetById",
            type: "GET",
            data: {
                id: id
            },
            success: function (response) {

                //JoiningDate
                var jdate = new Date(parseInt(response.JoiningDate.substr(6)));
                var day = ("0" + jdate.getDate()).slice(-2);
                var month = ("0" + (jdate.getMonth() + 1)).slice(-2);
                var JoinDates = jdate.getFullYear() + "-" + (month) + "-" + (day);

                //PRL Date
                var prldate = new Date(parseInt(response.PRLDate.substr(6)));
                var dayprl = ("0" + prldate.getDate()).slice(-2);
                var monthprl = ("0" + (prldate.getMonth() + 1)).slice(-2);
                var PrlDates = prldate.getFullYear() + "-" + (monthprl) + "-" + (dayprl);

                //Order Date
                var orderdate = new Date(parseInt(response.OrderDate.substr(6)));
                var dayorder = ("0" + orderdate.getDate()).slice(-2);
                var monthorder = ("0" + (orderdate.getMonth() + 1)).slice(-2);
                var orderDates = orderdate.getFullYear() + "-" + (monthorder) + "-" + (dayorder);

                //JobPermanentDate
                var JobPermanentdate = new Date(parseInt(response.OrderDate.substr(6)));
                var dayJobPermanent = ("0" + JobPermanentdate.getDate()).slice(-2);
                var monthJobPermanent = ("0" + (JobPermanentdate.getMonth() + 1)).slice(-2);
                var JobPermanentDates = JobPermanentdate.getFullYear() + "-" + (monthJobPermanent) + "-" + (dayJobPermanent);





                $("#Id").val(response.Id);

                $("#JoiningDate").val(JoinDates);
                $("#JoiningRankId").prop("selectedIndex", response.JoiningRankId);
                $("#OfficeEmail").val(response.OfficeEmail);
                $("#PostingTypeId").prop("selectedIndex", response.PostingTypeId);
                $("#DesignationTypeId").prop("selectedIndex", response.DesignationTypeId);
                $("#OfficeType").prop("selectedIndex", response.OfficeType);

                //$("#PostingTypeId").prop("selectedIndex", response.PostingTypeId);
                $("#PostingId").prop("selectedIndex", response.PostingId);
                $("#DesignationClassId").prop("selectedIndex", response.DesignationClassId);
                $("#DesignationId").prop("selectedIndex", response.DesignationId);
                $("#GradationNumber").val(response.GradationNumber);

                $("#GradationTypeId").prop("selectedIndex", response.GradationTypeId);
                $("#EmployeeTypeId").prop("selectedIndex", response.EmployeeTypeId);
                $("#JobCategoryId").prop("selectedIndex", response.JobCategoryId);
                $("#PostResponsibilityId").prop("selectedIndex", response.PostResponsibilityId);
                $("#RecruitPromoId").prop("selectedIndex", response.RecruitPromoId);
                $("#PromtionNatureId").prop("selectedIndex", response.PromtionNatureId);
                $("#Section").val(response.Section);
                $("#GradationYear").val(response.GradationYear);
                $("#JobPermanentDate").val(JobPermanentDates);
                $("#DeppostingTypeId").prop("selectedIndex", response.DeppostingTypeId);
                $("#SectionAt").val(response.SectionAt);
                $("#PRLDate").val(PrlDates);
                $("#Batch").val(response.Batch);
                $("#CadreId").prop("selectedIndex", response.CadreId);
                $("#OrderNumber").val(response.OrderNumber);
                $("#OrderDate").val(orderDates);
                $("#Remarks").val(response.Remarks);
                $("#PostingDetailsId").prop("selectedIndex", response.PostingDetailsId);


                $("[name='PostingTypeId']").prop("selectedIndex", response.PostingTypeId);
              
            },
            error: function () {
            }
        });
    }
</script>