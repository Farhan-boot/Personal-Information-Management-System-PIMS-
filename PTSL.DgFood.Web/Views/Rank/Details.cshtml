@model PTSL.DgFood.Web.Model.RankVM
@{
    ViewBag.Title = "Details";
}


<div class="card">
    <div class="card-header">
        <h4>Rank Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label for="inputRank" style="font-size:15px;">Rank Name: <b>@Model.RankName</b></label>
                @*<input type="text" class="form-control col-md-6" id="RankName" name="RankName" value="@Model.RankName" placeholder="Enter Rank Name">*@
            </div>
            <div class="form-group">
                <label for="inputRankBangla" style="font-size:15px;">Rank Name Bangla: <b>@Model.RankNameBangla</b></label>
            </div>

        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/Rank/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

